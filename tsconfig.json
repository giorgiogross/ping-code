{
  "exclude": [
    "./webpack.config.js",
    "./webpack.config.extension.js",
    "./webpack.config.app.js",
    "dist",
  ],
	"compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "esnext"],
    "outDir": "dist",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "react",
		"sourceMap": true,
		"rootDir": "src",
    "esModuleInterop": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
		"strict": false,
	}
}
