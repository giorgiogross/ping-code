{
  "exclude": [
    "./webpack.config.js",
    "./webpack.config.ext.js",
    "dist",
  ],
	"compilerOptions": {
    "target": "es6",
		"module": "commonjs",
    "lib": ["dom", "es6"],
    "outDir": "dist",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "react-jsx",
		"sourceMap": true,
		"rootDir": "src",
    "esModuleInterop": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noImplicitAny": false,
    "strictNullChecks": false,
		"strict": false,
	}
}
